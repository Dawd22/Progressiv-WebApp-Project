{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
          "name": "app",
          "installMode": "prefetch",
          "resources": {
            "files": [
              "/favicon.ico",
              "/index.html",
              "/main.*.js",
              "/polyfills.*.js",
              "/runtime.*.js",
              "/styles.*.css"
            ]
          }
        },
        {
            "name": "firestore-data",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
              "urls": [
                "https://firestore.googleapis.com/v1/projects/todoapplication-f75d9/databases/(default)/documents/**"
              ],
              "strategy": "performance"
            }
          }
        ],
        "dataGroups": [
          {
            "name": "firestore-data",
            "urls": [
                "https://firestore.googleapis.com/v1/projects/todoapplication-f75d9/databases/(default)/documents/**"
            ],
            "cacheConfig": {
              "maxSize": 100,
              "maxAge": "1d",
              "timeout": "30s",
              "strategy": "freshness"
            }
          }
        ]
      }